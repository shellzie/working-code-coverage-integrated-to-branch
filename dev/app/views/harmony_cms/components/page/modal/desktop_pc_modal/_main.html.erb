<div class="desktop-pc-modal">

    <div class="headings-title">
		<%= render_child_component_if_exists_with_name(component, "cq-headings") %>
    </div>

    <div class="modal-container heading-desc">
		<%= render_child_component_if_exists_with_name(component, "cq-text") %>
    </div>

	<div>
   		<div class="card-container">
    		<div class="modal-container">
        		<div class="row">
            		<div class="span2">                		
						<%= render_child_component_if_exists_with_name(component, "cq-label") %>
            		</div>
                    <div class="span4" style="display:inline-block;float:left;">                        
						<%= render_child_component_if_exists_with_name(component, "cq-add-card") %>
                    </div>
                    <div class="span4" style="display:inline-block;">                        
						<%= render_child_component_if_exists_with_name(component, "cq-upgrade-card") %>
                    </div>            
        		</div>
                <div class="row cqcard-footer" style="display:block;">
					<%= render_child_component_if_exists_with_name(component, "cq-card-footer") %>                 
                </div>
    		</div>
		</div>

		<div class="modal-container">
            <div class="faq-sec">            
				<%= render_child_component_if_exists_with_name(component, "cq-text-footer") %> 
            </div>
            <div class="pricing-terms">
                <div style="display:inline-block;">
					<%= render_child_component_if_exists_with_name(component, "cq-text-disclosures") %>                    
                </div>
            </div>
		</div>
	</div>
</div>